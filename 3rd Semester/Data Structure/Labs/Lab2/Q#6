//Write a C++ program to deletes ITEM from Bottom of the array. Print all elements of the array.

#include <iostream>
using namespace std;

int main() {
    int arr[6] = {22, 34, 21, 4, 3}; // Declare with extra space
    cout << "Array Elements before Deletion:" << endl;
    for(int i = 0; i < 5; i++) {
        cout << arr[i] << "\t";
    }

    arr[5] = 0; // Set the last element to 0 (or any other value)

    cout << "\nArray Elements after Deletion from Bottom:" << endl;
    for(int i = 0; i < 5; i++) {
        cout << arr[i] << "\t";
    }

    return 0;
}
