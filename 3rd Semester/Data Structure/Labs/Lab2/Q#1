//Write a C++ program to print largest value of array.

#include<iostream>
using namespace std;
int main(){
	int num [] = {-2,15,32,-23,12};
		int size = 5;
	int smallest = INT_MAX;
	for(int i = 0;i<size;i++){
			smallest = min(num[i] , smallest);

	}
	cout<<"smallest ="<<smallest<<endl;
	return 0;
}
