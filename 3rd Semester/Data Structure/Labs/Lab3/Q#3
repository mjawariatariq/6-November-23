#include <iostream>
using namespace std;

int main() {
    int n, sum = 0;
    cin >> n;
    int matrix[n][n];

    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            cin >> matrix[i][j];

    for (int i = 0; i < n; i++)
        sum += matrix[i][i];

    cout << sum << endl;
    return 0;
}
